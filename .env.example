# AI Agent Discovery - Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/ai_agents
# For development with SQLite: DATABASE_URL=sqlite:///./ai_agents.db

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# Frontend Configuration
FRONTEND_URL=http://localhost:3000

# Ingestion Configuration
INGESTION_DELAY=1
INGESTION_MAX_PAGES=5
INGESTION_BATCH_SIZE=100

# Optional: API Keys for enhanced ingestion
# GITHUB_TOKEN=your_github_token_here
# HUGGINGFACE_TOKEN=your_hf_token_here
# RAPIDAPI_KEY=your_rapidapi_key_here

# Production Settings
DEBUG=false
LOG_LEVEL=INFO
